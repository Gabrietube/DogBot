import random


def gen_pass(pass_length):
    elements = "+-/*!&$#?=@<>"
    password = ""

    for i in range(pass_length):
        password += random.choice(elements)

    return password

def gen_image():
    images = ["https://assets.puzzlefactory.com/puzzle/330/365/original.webp", "https://assets.puzzlefactory.com/puzzle/341/513/original.jpg", "https://assets.puzzlefactory.com/puzzle/292/623/original.webp"]
    
    return random.choice(images)

    
